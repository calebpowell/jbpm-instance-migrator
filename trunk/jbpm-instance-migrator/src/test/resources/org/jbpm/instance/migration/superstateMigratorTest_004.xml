	<start-state name="Start">
		<transition to="superDuperStateNode/superStateNode/Saving" name="save"></transition>
	</start-state>
	
	<super-state name="superDuperStateNode">
		<super-state name="superStateNode">
			<node name="Saving">
		      <event type="node-enter">
		      </event>
		      <transition name="" to="Saved2"></transition>
		  	 </node>
		
			<state name="Saved2">
				<transition to="../../Deleted" name="cancel">
				</transition>
			</state>
		</super-state>
	</super-state>
	
	<end-state name="Deleted"></end-state>


</process-definition>